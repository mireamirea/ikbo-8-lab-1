#include <stdio.h>
#include <stdlib.h>

typedef struct{
int x;
char Masive[255];
float y;}s;

int main(int x, const char * Masive[])
{
    FILE *file;
    int i = 0;
    s *dynamic = malloc(sizeof(s));

    if(x != 2){
        printf("Go Console esli ti mujik");
        return 0;
    }
    if ((file = fopen(Masive[1], "r+b")) == NULL){
        printf("Go Console esli ti mujik");
        return 0;
    }
    while(!feof(file)){
        fread(dynamic, sizeof(s), 1, file);
        printf("%d %s %f", dynamic -> x, dynamic -> Masive, dynamic -> y);
        i++;
    }
    fseek(file, 0, SEEK_END);
    printf("Elements: %d \n", i);
    printf("Size: %ld", ftell(file));
    fclose(file);
}
